Query: Create 3 ninjas that belong to the first dojo

INSERT INTO ninjas (first_name, last_name, age, dojo_id)
VALUES ('Katelyn', 'Chambers, 25, 1), ('Chani', 'Hodges', 30, 1), ('Ki', 'Clayburn', 26, 1);

